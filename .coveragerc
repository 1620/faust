[run]
branch = 1
cover_pylib = 0
include=*faust/*
omit = t.*

[report]
omit =
    */python?.?/*
    */site-packages/*
    */pypy/*
    */faust/__main__.py
    */faust/app/_attached.py
    */faust/types/*
    */faust/assignor/*
    */faust/fixups/*
    */faust/web/apps/*
    */faust/utils/kafka/*
    */faust/cli/_env.py
    */faust/cli/agents.py
    */faust/cli/model.py
    */faust/cli/models.py
    */faust/cli/reset.py
    */faust/cli/send.py
    */faust/cli/tables.py
    */faust/cli/worker.py
